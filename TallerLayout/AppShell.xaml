<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TallerLayout.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TallerLayout"
    Shell.FlyoutBehavior="Disabled"
    Title="TallerLayout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent Title="StackLayoutDemo" ContentTemplate="{DataTemplate local:StackLayoutDemo}" />
    <ShellContent Title="FlexLayoutPage" ContentTemplate="{DataTemplate local:FlexLayoutPage}" />

</Shell>
